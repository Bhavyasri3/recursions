// Online C++ compiler to run C++ program online
//factorial on n numbers using recursion 
#include <iostream>
using namespace std;
int fact(int n){
    if (n==0){
        return 1;
    }
    int small=fact(n-1);
    return n*small;
}
int main() {
    
   int n;
   cin>>n;
   cout<<fact(n);
    return 0;
}
